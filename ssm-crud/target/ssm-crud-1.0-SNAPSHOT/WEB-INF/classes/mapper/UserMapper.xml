<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hui.crud.dao.UserMapper">
    <insert id="reg" parameterType="com.hui.crud.pojo.User">
        insert into t_user(t_username,t_password,t_email,t_creationtime,t_updatetime) values
        (#{username},#{password},#{email},#{creationtime},#{updatetime})
    </insert>
</mapper>